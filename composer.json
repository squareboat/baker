{
    "name": "squareboat/baker",
    "description": "A set of helpers for baking your Laravel Project.",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "generator", "baker"],
    "authors": [
        {
            "name": "Amit Gupta",
            "email": "akaamitgupta@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "5.3.*|5.4.*|5.5.*",
        "illuminate/filesystem": "5.3.*|5.4.*|5.5.*",
        "symfony/console": "~3.1|~3.2|~3.3"
    },
    "autoload": {
        "psr-4": {
            "SquareBoat\\Baker\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "SquareBoat\\Baker\\BakerServiceProvider"
            ]
        }
    }
}